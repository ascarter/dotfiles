          "string.regex": {
            "font_weight": 600
          },
          "string.special": {
            "font_weight": 600
          },
          "tag": {
            "color": "#2A72E5",
            "font_weight": 600
          },
          "text.literal": {
            "font_style": "italic"
          },
          "title": {
            "font_weight": 600
          }
        },
        "players": [
