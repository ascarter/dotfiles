        "text.accent": "#2A72E5",
        "vim.normal.background": "#F5F5F5",
        "vim.insert.background": "#2A72E540",
        "vim.replace.background": "#2A72E540",
        "vim.visual.background": "#D1D1D1",
        "vim.helix_normal.background": "#F5F5F5",
        "vim.helix_select.background": "#D1D1D1",
        "syntax": {
          "comment": {
            "color": "#595959",
            "font_style": "italic"
          },
          "constant": {
            "font_style": "italic"
          },
          "emphasis": {
            "font_weight": 400
          },
          "emphasis.strong": {
            "font_weight": 700
          },
          "function": {
            "font_weight": 600
          },
