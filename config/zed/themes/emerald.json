{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "Emerald",
  "author": "Andrew Carter <ascarter@icloud.com>",
  "themes": [
    {
      "name": "Emerald Light",
      "appearance": "light",
      "style": {
        "background": "#EBEBEB",
        "foreground": "#141414",
        "accent": "#E2C15A",
        "editor.active_line_background": "#E0E0E0",
