      "style": {
        "background": "#FFFFFF",
        "editor.background": "#FFFFFFFF",
        "editor.foreground": "#1C1C1EFF",
        "editor.gutter.background": "#FFFFFFFF",
        "editor.gutter.foreground": "#6E6E73FF",
        "editor.active_line_number": "#1C1C1EFF",
        "editor.active_line.background": "#6F82B320",
        "editor.line_number": "#C7C7CCFF",
        "editor.document_highlight.bracket_background": "#FF8F1FA6",
        "search.match_background": "#FF8F1FFF",
        "text": "#1C1C1EFF",
        "text.accent": "#B45309FF",
        "text.disabled": "#C7C7CCFF",
        "text.muted": "#6E6E73FF",
        "created": "#6F82B3FF",
        "deleted": "#D70015FF",
