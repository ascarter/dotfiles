            "font_weight": 600
          },
          "preproc": {
            "color": "#6E6E73FF",
            "font_style": "italic"
          },
          "punctuation.list_marker": {
            "font_weight": 600
          },
          "string": {
            "color": "#1C2B5AFF",
            "font_style": "italic"
          },
          "string.regex": {
            "font_style": "italic",
            "font_weight": 600
          },
          "string.special": {
            "font_style": "italic",
            "font_weight": 600
          },
          "tag": {
            "color": "#1C2B5AFF",
            "font_weight": 600
          },
          "text.literal": {
            "font_style": "italic"
          },
          "title": {
            "font_weight": 600
          }
        },
        "players": [
          {
            "cursor": "#1C2B5AFF",
            "selection": "#6F82B340"
          }
        ]
      }
    },
