        "vim.visual_line.background": "#6F82B340",
        "vim.visual_block.background": "#6F82B340",
        "vim.helix_normal.background": "#C7C7CCA6",
        "vim.helix_select.background": "#6F82B340",
        "syntax": {
          "comment": {
            "color": "#6E6E73FF",
            "font_style": "italic"
          },
          "constant": {
            "font_style": "italic"
          },
          "emphasis": {
            "font_weight": 400
          },
          "emphasis.strong": {
            "font_weight": 700
          },
          "function": {
            "color": "#1C2B5AFF"
          },
          "link_text": {
            "color": "#1C2B5AA6"
